{
  "name": "foundryvtt-rest-api-relay",
  "version": "0.0.1",
  "description": "Message relay server for the foundryvtt-rest-api Foundry Module",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "cross-env PORT=3010 dotenv -e .env tsx watch src/index.ts",
    "db:migrate": "tsx src/models/init.ts",
    "local": "cross-env DB_TYPE=memory PORT=3010 tsx watch src/index.ts",
    "postinstall": "npx puppeteer install"
  },
  "author": "ThreeHats",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@clerk/backend": "^1.24.3",
    "@stripe/stripe-js": "^5.10.0",
    "axios": "^1.8.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "handlebars": "^4.7.8",
    "ioredis": "^5.6.0",
    "jsdom": "^26.0.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.13.3",
    "prettier": "^3.5.2",
    "prom-client": "^15.1.3",
    "puppeteer": "^24.4.0",
    "sequelize": "^6.37.6",
    "sqlite3": "^5.1.7",
    "stripe": "^17.7.0",
    "winston": "^3.17.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.5",
    "@types/node-fetch": "^2.6.12",
    "@types/pg": "^8.11.11",
    "@types/ws": "^8.5.14",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "fvtt-types": "npm:@league-of-foundry-developers/foundry-vtt-types@12.331.3-beta",
    "sequelize-cli": "^6.6.2",
    "tsx": "^4.19.3",
    "typescript": "~5.7.3"
  }
}
