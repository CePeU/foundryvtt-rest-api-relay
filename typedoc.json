{
  "entryPoints": ["src/index.ts"],
  "out": "docs/md/api",
  "plugin": ["typedoc-plugin-markdown"],
  "theme": "markdown",
  "readme": "none",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "hideGenerator": true,
  "disableSources": false,
  "sourceLinkTemplate": "https://github.com/JustAnotherIdea/foundryvtt-rest-api-relay/blob/main/{path}#L{line}",
  "gitRevision": "main",
  "outputFileStrategy": "modules",
  "fileExtension": ".md",
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "categoryOrder": [
    "Routes",
    "Middleware", 
    "Models",
    "Utils",
    "Types"
  ],
  "sort": ["source-order"],
  "kindSortOrder": [
    "Document",
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "EnumMember",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Accessor",
    "Method",
    "Parameter",
    "TypeParameter",
    "TypeLiteral",
    "CallSignature",
    "ConstructorSignature",
    "IndexSignature",
    "GetSignature",
    "SetSignature"
  ]
}
